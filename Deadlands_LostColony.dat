  <thing id="modDLCSpeedS" name="Speed" description="Increases the vehicle’s Top Speed by 50 MPH." compset="Modificatn">
    <fieldval field="grModsUsed" value="1"/>
    <fieldval field="grCost" value="100000"/>
    <usesource source="DLC_Core"/>
    <tag group="ModType" tag="Starship"/>
    <tag group="User" tag="SizeCost"/>
    <eval phase="PreTraits" priority="5500"><![CDATA[var speed as number

speed = container.parent.field[vhTopSpeed].text 

if (field[stackQty].value > 1) then 
  speed += 50 * field[stackQty].value
else      
  speed += 50
  endif
container.parent.field[vhTopSpeed].text = speed]]></eval>
    </thing>
  <thing id="modDLCTargetSysS" name="Targeting System" description="A computer linked to the ship’s weapons compensates for movement, range, multi-actions, and so forth, negating up to two points of Shooting penalties from those sources." compset="Modificatn" summary="Negates up to 2 points of penalties for movement, range, multi-actions, and so forth.">
