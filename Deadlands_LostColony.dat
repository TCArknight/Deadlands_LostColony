    </thing>
  <thing id="edgDLCBansheeScr" name="Banshee Screams" description="The Guardian lets out an ear-piercing shriek that mimics Banshee’s howling winds. The character spends a Benny and makes an Athletics roll to issue a terrifying scream in the shape of a Cone Template. Those within must make a Vigor roll at −2 or be Stunned and have the Hard of Hearing (Major) Hindrance for the next hour." compset="Edge">
    <usesource source="DLC_Core"/>
    <tag group="MinRank" tag="3"/>
    <tag group="EdgeType" tag="Guardian" name="Guardian"/>
    <pickreq thing="edgDLCGuardian"/>
    </thing>
  <thing id="edgDLCBloodSong" name="Blood Song" description="The Guardian spends a Benny and sings a dramatic song of fierce tribal victories long past. For the next five rounds, she adds +2 to all her damage rolls." compset="Edge">
    <usesource source="DLC_Core"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Guardian" name="Guardian"/>
    <pickreq thing="edgDLCGuardian"/>
    </thing>
  <thing id="edgDLCMartyr" name="Martyr" description="As an action, a Guardian can touch another and transfer one or more of their Wounds to himself. He doesn’t take on any other states or effects, such as poison, disease, Shaken, or Stunned conditions. He can take on enough Wounds to Incapacitate himself, but no more." compset="Edge">
    <usesource source="DLC_Core"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Guardian" name="Guardian"/>
    <pickreq thing="edgDLCGuardian"/>
    </thing>
  <thing id="edgDLCSupernatAtt" name="Supernatural Attribute" description="This Edge permanently improves any one of the hero’s attributes two die types (a d6 becomes a d10, a d12 becomes a d12+2, and so on). This Edge may be taken up to five times, but only once for each attribute. This increases the Trait’s limit and the limit of any linked skills a like amount." compset="Edge">
    <fieldval field="usrCandid1" value="component.Attribute &amp; !(thingid.attrSup|Helper.Supernatural)"/>
    <usesource source="DLC_Core"/>
    <tag group="Helper" tag="Supernatural" name="Supernatural Attribute"/>
    <tag group="EdgeType" tag="Guardian" name="Guardian"/>
    <tag group="MinRank" tag="0"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[
    ~apply the +2 modifier to each selected skill
    if (field[usrChosen1].ischosen <> 0) then
      field[usrChosen1].chosen.field[trtCreate].value += 2
	  perform field[usrChosen1].chosen.assign[Helper.Supernatural]
      endif]]>
      <before name="Calc trtFinal"/>
      </eval>
    <pickreq thing="edgDLCGuardian"/>
    </thing>
  <thing id="edgDLCStoneSkin" name="Stone Skin" description="The Guardian’s Toughness permanently improves by +2." compset="Edge">
    <usesource source="DLC_Core"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Guardian"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[perform #traitadjust[trTough,+,2,"Stone Skin"]]]></eval>
    <pickreq thing="edgDLCGuardian"/>
    </thing>
  <thing id="edgDLCTannisBomb" name="Tannis Bomb" description="As an action, the Guardian may spend a Benny to charge a piece of tannis with lethal energy. This requires at least eight ounces of tannis. Using larger samples has no additional effect. The “bomb” retains its charge indefinitely and detonates only when the Guardian wills it to (a free action). It can be thrown like a grenade (Range 5/10/20, Medium Blast Template) for 2d6 damage (3d6 with a raise on the Athletics roll)." compset="Edge" summary="Create an explosive with 8+ ounces of Tannis. Can be used as a grenade (5/10/20; 2d6 MBT (3d6 on raise)">
    <usesource source="DLC_Core"/>
    <tag group="MinRank" tag="3"/>
    <tag group="EdgeType" tag="Guardian" name="Guardian"/>
    <pickreq thing="edgDLCGuardian"/>
    </thing>
  <thing id="edgDLCWindSong" name="Wind Song" description="The Guardian may communicate with someone she knows, anywhere on the surface of Banshee. The recipient hears the hero’s voice in the wind, distant but clear." compset="Edge" summary="Communicate with someone you know anywhere on the surface of Banshee.">
    <usesource source="DLC_Core"/>
    <tag group="MinRank" tag="3"/>
    <tag group="EdgeType" tag="Guardian" name="Guardian"/>
    <pickreq thing="edgDLCGuardian"/>
    </thing>
  <thing id="rpDLCBansheeChamp" name="Banshee&apos;s Champion" description="All Guardians are Driven (Major) to protect Banshee and its creatures." compset="RacialProp">
