    </thing>
  <thing id="rpDLCResiliency" name="Resiliency" description="The life energy of a Guardian is magnified by its tether to Banshee. This gives her +1 Toughness and +2 to Vigor rolls to resist Fatigue, poison, and disease." compset="RacialProp" summary="+1 Toughness; +2 to Vigor rolls to resist Fatigue, poison, and disease">
    <usesource source="DLC_Core"/>
    <bootstrap thing="hinDriven">
      <autotag group="User" tag="HindMajor"/>
      </bootstrap>
    </thing>
